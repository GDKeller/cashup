<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.cashup" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CashUp</name>
    <description>
        Count up your wage earnings in real time.
    </description>
    <author email="grant@kellerdigital.com" href="http://kellerdigital.com">
        Grant Keller
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
